<?xml version="1.0" encoding="UTF-8"?>
<!--引入xml的约束设置文件-->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--通过配置，告诉Spring的IoC容器，你需要帮我管理哪一个类的对象-->
    <bean name="helloWorld,a,b,c"  class="hello_setter.HelloWorld">
        <property name="username" value="Kitty"/>
        <property name="age" value="11"/>
    </bean>
    <bean id="helloWorld22"  class="hello_setter.HelloWorld">
        <property name="username" value="Kitty2"/>
        <property name="age" value="112"/>
    </bean>

</beans>